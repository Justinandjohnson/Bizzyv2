    "@types/node": "^20.16.10",
    "@types/react": "^18.2.0",
    "@types/styled-components": "^5.1.34",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.0.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
