{
  "type": "module",
  "name": "enhanced-brainstormer",
  "version": "1.0.0",
  "description": "AI-powered business idea brainstorming tool",
  "main": "dist/server.js",
  "scripts": {
    "dev:frontend": "next dev",
    "dev:backend": "tsx watch backend/server.ts",
